{
    "openapi": "3.0.0",
    "info": {
        "title": "BoomBang API",
        "description": "BoomBang API - Swagger\r\n *    To be able to authorize api write in Authorize: Bearer {token}",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api/v1/",
            "description": ""
        }
    ],
    "paths": {
        "/auth/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Sign in",
                "description": "Login by email, password",
                "operationId": "authLogin",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "Email",
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "description": "Password",
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Wrong credentials response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Sorry, wrong email address or password. Please try again"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/packets": {
            "get": {
                "tags": [
                    "Utils"
                ],
                "summary": "Game Packets",
                "description": "Game Packets",
                "operationId": "getGamePackets",
                "responses": {
                    "200": {
                        "description": "Results limit exceeded",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": ""
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/sceneries/publics/all": {
            "get": {
                "tags": [
                    "Sceneries"
                ],
                "summary": "Get Public Esceneries",
                "description": "Get Public Esceneries",
                "operationId": "getPublicEsceneries",
                "responses": {
                    "200": {
                        "description": "Results limit exceeded",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": ""
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "passport": {
                "type": "apiKey",
                "description": "Laravel passport apiKey security.",
                "name": "Authorization",
                "in": "header",
                "scheme": "https",
                "flows": {
                    "password": {
                        "authorizationUrl": "http://localhostoauth/authorize",
                        "tokenUrl": "http://localhostoauth/token",
                        "refreshUrl": "http://localhosttoken/refresh",
                        "scopes": []
                    }
                }
            }
        }
    }
}